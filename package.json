{
  "devDependencies": {
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "2.5.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npx webpack --config webpack.config.js",
    "lint": "npx eslint -c .eslintrc.json JS_DOM/todo_list/*.js",
    "prettier": "npx prettier --write --ignore-unknown JS_DOM/todo_list/*.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0"
  }
}

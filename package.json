{
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "eslint": "^8.8.0",
    "prettier": "2.5.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npx webpack --config webpack.config.js",
    "lint": "npx eslint -c .eslintrc.json JS_PROJECT_12/todo/*.js",
    "prettier": "npx prettier --write --ignore-unknown JS_PROJECT_12/todo/*.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier"
    ]
  }
}
